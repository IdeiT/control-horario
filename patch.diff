index 0000000..e14a4d8
--- /dev/null
+++ b/ProyectoControlHorario/backend/springBootRun.sh
@@ -0,0 +1,20 @@
+#!/bin/bash
+
+echo "Cargando variables desde .env ..."
+
+# Comprobar si .env existe
+if [ ! -f ".env" ]; then
+    echo "No se encontrÃ³ el archivo .env"
+    exit 1
+fi
+
+# Cargar las variables del archivo .env
+set -a
+source .env
+set +a
+
+echo "Variables cargadas correctamente."
+echo "Lanzando mvn spring-boot:run..."
+echo
+
+mvn spring-boot:run
